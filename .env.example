
# .env.example

# Supabase configuration
VITE_SUPABASE_URL=https://your-supabase-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-public-api-key

# OAuth Secrets (Supabase)
SUPABASE_AUTH_GOOGLE_CLIENT_ID=your-google-client-id
SUPABASE_AUTH_GOOGLE_CLIENT_SECRET=your-google-client-secret
SUPABASE_AUTH_TWITTER_CLIENT_ID=your-twitter-client-id
SUPABASE_AUTH_TWITTER_CLIENT_SECRET=your-twitter-client-secret

# Firebase Configuration (if needed)
NEXT_PUBLIC_FIREBASE_API_KEY=your-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Application URLs
VITE_APP_URL=your-app-url # The URL where your app is hosted
VITE_REDIRECT_URL=your-redirect-url # OAuth redirect URL
